{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "rsync.syncToController",
      "type": "process",
      "command": "rsync",
      "args": [
        "-avzR",
        "--delete",
        "--exclude",
        ".git",
        "--filter",
        ":- .gitignore",
        "-e",
        "ssh -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null -i ~/.ssh/science.ed25519 -p 8822",
        ".",
        "user@127.0.0.1:/opt/projects/generally_intelligent/"
      ],
      "presentation": {
        "echo": true,
        "reveal": "never",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": false,
        "clear": false,
        "close": false
      }
    },
    /*
      Drafty tasks
      - core completion commands: execute, cycle_response_(next|previous), accept_completion
      - (open|close)_inteface commands: toggle the janky feedback interface file
      - open_chat: opens pseudo-chat text file for interactively refining prompts for a generation
      - (sync|remove|list)_hightlights are... idk, internal maybe
      - watch_file is external boltons
    */
    {
      "label": "drafty.execute",
      "args": [
        "standalone/llms/llms/agents/cli/drafty_cli.py",
        "execute",
        "${file}",
        "${lineNumber}",
        "0"
      ],
      "type": "process",
      "command": ".venv/bin/python",
      "presentation": {
        "reveal": "never",
        "panel": "shared"
      },
      "problemMatcher": [],
      "echoCommand": true
    },
    {
      "label": "drafty.accept_completion",
      "args": [
        "standalone/llms/llms/agents/cli/drafty_cli.py",
